    <command interpreter="python">data_manager_pulsar_nodes_monitor.py "${out_file}"</command>
    <inputs>
      <param name="min" type="integer" value="0" label="Min. Instances" />
      <param name="max" type="integer" value="1" label="Max. Instances" />
    </inputs>
    <outputs>
        <data name="out_file" format="data_manager_json" />
    </outputs>
</tool>
